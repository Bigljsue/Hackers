<Page x:Class="WPF_HackersList.Windows.Pages.MenuPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_HackersList.Windows.Pages"
        mc:Ignorable="d"
        Title="MenuPage">
    <Grid>
        <StackPanel VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Background="#CFCFCF">
            <Button 
            Name="Button_OpenHackersPage" 
            HorizontalAlignment="Center"
            Margin="10,10,10,10"  
            Padding="0,0,0,0" 
            Width="95"
            Height="95"
            Click="Button_OpenHackersPage_Click">
                <Image Source="/Images/Hacker.png"/>
            </Button>
        </StackPanel>
    </Grid>
</Page>
